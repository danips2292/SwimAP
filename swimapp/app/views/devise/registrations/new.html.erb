<%= image_tag 'bg.jpeg', height:'100', :id => "bg" %>
<div class="outer_frame">
  <div class="middle_frame">
     <div class ="login">
      <%= image_tag 'logo.png', height:'50', class:'logo'%>
      <div class='form_container'>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

    <table>
    <tr>
    <td><%= "Correo Electrónico" %></td>
    <td><%= f.email_field :email, autofocus: true %></td>
    </tr>



    <tr>
    <td><%= "Contraseña"%></td>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %></td>
    <td>
    <%= f.password_field :password, autocomplete: "off" %></td>
  </tr>



    <tr>
    <td><%= "Repita Contraseña" %></td><td>
    <%= f.password_field :password_confirmation, autocomplete: "off" %></td>
    </tr>

    <tr><td></td>
    <td>
    <%= f.submit "Crear" %>
    </td>
    </tr>
<% end %>
</table>

<%= render "devise/shared/links" %>
</div>
</div>
</div>
</div>
