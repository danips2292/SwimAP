<h1>Chat</h1>
<table>
<% @users.each_with_index do |user, index| %>
    <tr>
      <td><%= index +=1 %></td>
      <td><%= user.full_name %></td>
      <td>
        <%= link_to "Send Message", "#", class: "btn btn-success btn-xs start-conversation",
                    "data-sid" => @current_user, "data-rip" => user.id %>
      </td>
    </tr>
<% end %>
</table>